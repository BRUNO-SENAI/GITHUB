programa {
  funcao inicio() {
    //5 – Leia dois números e verifique se eles são iguais ou diferentes
    escreva("Digite um numero: ")
    real numero1
    leia(numero1)

    escreva("Digite outro numero: ")
    real numero2
    leia(numero2)

    se(numero1 == numero2)
    escreva("Numeros iguais")

    se(numero1 != numero2)
    escreva("Numeros diferentes")
      
    }
    
  }
}
